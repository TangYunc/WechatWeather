<!--pages/main/main.wxml-->
<view class='contain'>


  <view class='today'>
    <view class='info'>
      <view class='temp'>{{today.wendu}}℃</view>
      <view class='weather'>{{today.todayInfo.type}} {{today.todayInfo.fengxiang}} {{today.todayInfo.fengli}}</view>
      <view>友情提示： {{today.ganmao}}</view>
      <view class='city'>{{city}}</view>
    </view>
  </view>


</view>